{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@images": "./imagens",
        "@components": "./src/components",
        "@utils": "./src/utils",
        "@actions": "./src/actions/Actions.js"
      }
    }]
  ]

}
